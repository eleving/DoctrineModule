{
    "name":        "eleving/doctrine-module",
    "description": "Zend Framework Module that provides Doctrine basic functionality required for ORM and ODM modules",
    "type":        "library",
    "license":     "MIT",
    "homepage":    "http://www.doctrine-project.org/",
    "keywords":    [
        "doctrine",
        "module",
        "zf"
    ],
    "authors":     [
        {
            "name": "Kyle Spraggs",
            "email": "theman@spiffyjr.me",
            "homepage": "http://www.spiffyjr.me/"
        },
        {
            "name": "Marco Pivetta",
            "email": "ocramius@gmail.com",
            "homepage": "http://marco-pivetta.com/"
        },
        {
            "name": "Guilherme Blanco",
            "email": "guilhermeblanco@hotmail.com"
        },
        {
            "name": "MichaÃ«l Gallego",
            "email": "mic.gallego@gmail.com",
            "homepage": "http://www.michaelgallego.fr"
        }
    ],
    "extra": {
        "zf": {
            "module": "DoctrineModule"
        }
    },
    "require": {
        "php":                               "^5.6 || ^7.0 || ^8.0",
        "cache/doctrine-adapter":            "^1.2.0",
        "doctrine/annotations":              "^2.0.0",
        "doctrine/cache":                    "^1.6.0",
        "doctrine/common":                   "^3.4.3",
        "doctrine/inflector":                "^2.0.6",
        "doctrine/orm":                      "^2.14.1",
        "doctrine/persistence":              "^3.1.3",
        "eleving/zend-stdlib":               "^2.7.12",
        "symfony/cache":                     "^v5.4.18",
        "symfony/console":                   "^3.0 || ^4.0 || ^5.0",
        "zendframework/zend-authentication": "^2.5.3",
        "zendframework/zend-cache":          "^2.7.1",
        "zendframework/zend-form":           "^2.9",
        "zendframework/zend-hydrator":       "^1.1",
        "zendframework/zend-mvc":            "^2.7.10",
        "zendframework/zend-paginator":      "^2.7",
        "zendframework/zend-servicemanager": "^2.7.6",
        "zendframework/zend-validator":      "^2.8.1"
    },
    "require-dev": {
        "phpunit/phpunit":                  "^4.8",
        "squizlabs/php_codesniffer":        "^2.6.2",
        "zendframework/zend-i18n":          "^2.7.3",
        "zendframework/zend-log":           "^2.9",
        "zendframework/zend-modulemanager": "^2.7.2",
        "zendframework/zend-serializer":    "^2.8",
        "zendframework/zend-session":       "^2.7.3",
        "zendframework/zend-test":          "^2.6.1 || ^3.0.1",
        "zendframework/zend-version":       "^2.5.1"
    },
    "replace": {
        "doctrine/doctrine-module": "1.2.1"
    },
    "suggest": {
        "doctrine/data-fixtures": "Data Fixtures if you want to generate test data or bootstrap data for your deployments",
        "zendframework/zend-mvc-console": "^1.1.10 if you are using ZF3"
    },
    "autoload": {
        "psr-0": {
            "DoctrineModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "DoctrineModuleTest\\": "tests/"
        }
    },
    "bin": [
        "bin/doctrine-module"
    ]
}
